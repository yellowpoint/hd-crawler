{
  "name": "crawler-aws",
  "type": "module",
  "git": "https://github.com/yellowpoint/hd-crawler.git",
  "scripts": {
    "reinstall": "rimraf node_modules && cnpm install",
    "api": "cross-env NODE_ENV=development nodemon --watch api --watch api-src --ext js api/index.js",
