    "modclean": "modclean --run"
  },
  "dependencies": {
    "@sparticuz/chromium": "^127.0.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "crawlee": "latest",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-ws": "^5.0.2",
    "glob": "^11.0.0",
    "gpt-tokenizer": "^2.4.0",
    "nodemon": "^3.1.7",
    "playwright": "^1.47.1",
    "playwright-core": "^1.47.1"
  }
}
